{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/app/*": [
        "./app/*"
      ],
      "@/types/*": [
        "./types/*"
      ],
      "@/queries/*": [
        "./sanity/queries/*"
      ],
      "@/styles/*": [
        "./styles/*"
      ],
      "@/components/*": [
        "./components/*"
      ],
      "@/context/*": [
        "./context/*"
      ],
      "@/public/*": [
        "./public/*"
      ],
      "@/helpers/*": [
        "./helpers/*"
      ],
      "@/sanity/*": [
        "./sanity/*"
      ],
      "@/data/*": [
        "./data/*"
      ],
      "@/lib/*": [
        "./lib/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
